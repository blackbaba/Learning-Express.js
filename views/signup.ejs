<% include _header %>

<h1>Sign up</h1>
<form action="/signup" method="POST">
  <input
    type="text"
    name="username"
    class="form-control"
    placeholder="username"
    required
    autofocus
  />
  <input
    type="password"
    name="password"
    placeholder="password"
    class="form-control"
    required
  />
  <input type="submit" value="Sign up" class="btn btn-primary btn-block" />
</form>

<% include _footer %>
